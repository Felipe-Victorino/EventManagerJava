classDiagram

class Room{
    -long id
    -String description
    -int capacity
}

class Session{
    -long id
    -DateTime datetime
    -Room room
    -LinkedList participants

    -registerParticipant(Participante part)
}

class Person{
    -String name
    -String cpf

    - abstract Card createCard()
}

class Participant{
    -long id
    -int avaliacao

    - Card createCard()
}

class Speaker{
    -long id

    - Card createCard()
}

class Subscription{
    -long id
    -Participant participant
    -Session session
    -Feedback feedback
}

class Event{
    -long id
    -Date date
}

class Feedback{
    -long id
    -int finalCapacity
    -int predictedCapacity
    -double rating
}

